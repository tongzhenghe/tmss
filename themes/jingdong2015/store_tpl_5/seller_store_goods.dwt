<script>
function handle(t){
	$(t).hide();
	$('#lessProp').show();
	$('[overfour=true]').show();
	$('.prop-attrs[overfour=true] .f-list').each(function(){
		if( $(this).find('li:last').position().top > 0 ){
			$(this).parent().parent().find('[option=more]').show();
		}	
	});
}
function lessProp(t){
    $(t).hide();
    $('#handle').show();
    $('[overfour=true]').hide();
}
function shuoqi(t){
	$(t).hide();
	$('#zhankai').show();
	$('.search_botttom').hide();
}
function zhankai(t){
	$(t).hide();
	$('#shuoqi').show();
	$('.search_botttom').show();
}
</script>
